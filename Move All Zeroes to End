// User function template for C++
class Solution {
  public:
    void pushZerosToEnd(vector<int>& arr) {
        // code here
        int n=arr.size();
        int nz=0;
        for(int i=0;i<n;i++){
            if(arr[i]!=0){
            std::swap(arr[nz],arr[i]);
            ++nz;
            }
        }
    }
